  "scripts": {
    "build": "gulp",
    "precommit": "npm version patch --no-git-tag-version && git add package.json package-lock.json",
    "test": "jest --coverage --detectOpenHandles",
    "patch2": "git stash; npm version patch; git stash pop;",
    "patch": "npm version patch",
    "minor": "npm --no-git-tag-version version minor",
    "major": "npm --no-git-tag-version version major",
    "prepare": "husky",
    "branch_name": "export CURR_BRANCH_NAME=$(git branch --show-current);",
    "dev-rd": "npm run branch_name; git stash; npm run patch; git push; npm run git_merge_push; git stash pop;",
    "git_merge_push": "git checkout dev-rd; git pull origin dev-rd; git merge $CURR_BRANCH_NAME; git push origin dev-rd; git checkout $CURR_BRANCH_NAME;"
  }
